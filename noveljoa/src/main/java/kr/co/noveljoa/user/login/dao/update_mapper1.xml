<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.noveljoa.user.login.updateMapper1">


<update id="passwordTemp" parameterType="kr.co.noveljoa.user.login.vo.PasswordTempVO">
