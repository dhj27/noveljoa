set 	pw = #{pw}
where 	id = #{id}
</update>
<update id="infoChange" parameterType="kr.co.noveljoa.user.login.vo.InfoChangeVO">
update	member
set 	phone = #{phone}
where 	id = #{id}
</update>
<update id="pwChange" parameterType="kr.co.noveljoa.user.login.vo.PasswordChangeVO">
update	member
set 	pw = #{pw}
where 	id = #{id}
</update>
<update id="imgChange" parameterType="kr.co.noveljoa.user.login.vo.ProfileImgVO">
update	member
set 	photo = #{photo}
where 	num_member = #{num_member}
</update>
</mapper>